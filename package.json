{
  "name": "voice-fe",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x",
    "npm": "6.14.x"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js",
    "test": "NODE_ENV=production vue-cli-service test:e2e --headless --env PORT=8080",
    "test:unit": "vue-cli-service test:unit"
  },
  "comments": {
    "mode test and cypress": "mode test has a special meaning in vue",
    "mode test and cypress_2": "buildpack sets NODE_ENV test in test step",
    "docker test command line": "docker run --rm -v $(pwd):/tmp/app registry.gitlab.com/acdh-oeaw/docker/herokuish-for-cypress/master /bin/herokuish buildpack test",
    "docker test command line explained": "needs a customized herokuish container for cypress'es dependencies",
    "docker test command line explained_2": "also cypress'es cache dir is adjusted",
    "AutoDevOps Settungs": "POSTGRES_ENABLED=false #we don't use a DB so don't prepare it"
  },
  "dependencies": {
    "config": "^3.3.3",
    "core-js": "^3.8.3",
    "http-cache-middleware": "^1.3.6",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "restana": "^4.8.1",
    "sax": "^1.2.4",
    "serve-static": "^1.14.1",
    "vue": "^2.6.10",
    "vue-matomo": "^3.14.0-0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.5.1",
    "vuetify": "^2.4.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/vue": "^5.6.1",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-e2e-cypress": "^4.5.11",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "jest": "^26.6.3",
    "sass": "^1.32.7",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.1.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    },
    "testRegex": "((\\.|/*.)(test))\\.js?$",
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
